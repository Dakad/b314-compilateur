  /* Comments are between delimiters */
  declare and retain
    /* Global declarations */
    i as integer[1];
    arena as square[27,27];

  while i[1] + 12 do
    skip
    set i[1] to i[1] + 1
  done

  by default do
    set i[1] to 2

    while i[1] * 12 do
      set i[1] to i[1] + 1
    done

  done
